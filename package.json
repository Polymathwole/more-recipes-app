{
  "name": "more-recipes-app",
  "version": "0.0.1",
  "description": "An app that allows users to provide recipes and receive reviews on them",
  "main": "app.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "npm run build && nyc --reporter=html --reporter=text mocha output/test",
    "prestart": "npm install -g babel-cli",
    "start": "npm run build && node output/app.js",
    "build": "babel server -d output",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "lint": "eslint server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Polymathwole/more-recipes-app.git"
  },
  "author": "Oluwaremi Oluwole",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Polymathwole/more-recipes-app/issues"
  },
  "homepage": "https://github.com/Polymathwole/more-recipes-app#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.5",
    "jsonwebtoken": "^8.0.1",
    "morgan": "^1.8.2",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.11.0",
    "sequelize-cli": "^3.0.0"
  },
  "engines": {
    "node": "8.1.2",
    "npm": "5.0.2"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.5.3",
    "nyc": "^11.2.1"
  }
}
